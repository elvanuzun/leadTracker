{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Leads Tracker",
    "action": {
        "default_popup": "index.html",
        "default_icon": "images/icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "commands": {
		"_execute_action": {
			"suggested_key": {
				"windows": "Ctrl+Shift+Y",
				"mac": "Command+Shift+Y"
			}
		}
	}
}